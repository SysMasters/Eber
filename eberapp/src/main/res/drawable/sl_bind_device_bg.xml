<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0.5dp" android:color="@color/trans5_white"/>
            <solid
                android:color="@color/trans5_white"/>
            <corners
                android:radius="8dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="0.5dp" android:color="#824e00"/>
            <solid
                android:color="#fff"/>
            <corners
                android:radius="8dp"/>
        </shape>
    </item>

</selector>
